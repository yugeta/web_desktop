            <b:loop var='post' values='data:posts'>
              <b:include name='articles' />
            </b:loop>

            <b:else/>
            # 記事一覧(index)
            <b:loop var='post' values='data:posts'>
              <b:include name='articles' />
            </b:loop>
          </b:if>

        <b:case value='item'/>
          <b:loop var='post' values='data:posts'>
            <b:include name='article' />
          </b:loop>

        <b:case value='static_page'/>
          <b:loop var='post' values='data:posts'>
            <b:include name='page' />
          </b:loop>

        <b:default/>
        <h1>Not found page.</h1>
          <data:blog.pageType/>
          <b:loop var='post' values='data:blogPosts'>
            <b:include name='articles' />
          </b:loop>

      </b:switch>
